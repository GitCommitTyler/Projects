<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>product_add</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>PetsServlet</display-name>
    <servlet-name>PetsServlet</servlet-name>
    <servlet-class>com.PetsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PetsServlet</servlet-name>
    <url-pattern>/list</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SearchJsp</servlet-name>
    <jsp-file>/search.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchJsp</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddPet</display-name>
    <servlet-name>AddPet</servlet-name>
    <servlet-class>com.AddPet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddPet</servlet-name>
    <url-pattern>/petadded</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddJsp</servlet-name>
    <jsp-file>/addpet.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddJsp</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>AddFilter</display-name>
    <filter-name>AddFilter</filter-name>
    <filter-class>filters.AddFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AddFilter</filter-name>
    <servlet-name>AddPet</servlet-name>
  </filter-mapping>
</web-app>